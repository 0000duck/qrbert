        <CopyToOutputDirectory>Always</CopyToOutputDirectory>
      </Content>
      <None Remove="Images\Qbert_logo_orange.png" />
      <Content Include="Images\Qbert_logo_orange.png">
        <CopyToOutputDirectory>Always</CopyToOutputDirectory>
      </Content>
      <None Remove="Images\Qbert_logo_white .png" />
      <Content Include="Images\Qbert_logo_white .png">
        <CopyToOutputDirectory>Always</CopyToOutputDirectory>
      </Content>
      <None Remove="Images\QRbertQRCode-Black.png" />
